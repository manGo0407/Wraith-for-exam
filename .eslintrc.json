{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2020

    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "import/no-dynamic-require": 0,
        "global-require": 0,
        "strict": 0,
        "no-console": 0,
        "no-unused-vars": 0,
        "max-len": 0
    }
}